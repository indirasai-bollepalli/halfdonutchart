{
    "id": "com.rohitchouhan.indirasai.halfdonutchart",
    "version": "1.0.0",
    "name": "halfdonutchart",
    "description": "Half donut chart",
    "newInstancePrefix": "halfdonutchart",
    "vendor": "Indirasai",
    "eula": "",
    "license": "MIT",
    "icon": "https://raw.githubusercontent.com/indirasai-bollepalli/halfdonutchart/main/icon.png",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-indirasai-sap-halfdonutchart",
            "url": "https://cdn.jsdelivr.net/gh/indirasai-bollepalli/halfdonutchart/halfdonutchart.js",
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
            "kind": "builder",
            "tag": "com-indirasai-sap-halfdonutchart-builder",
            "url": "https://cdn.jsdelivr.net/gh/indirasai-bollepalli/halfdonutchart/halfdonutchart_Builder.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "width": {
            "type": "integer",
            "default": 600
        },
        "height": {
            "type": "integer",
            "default": 420
        },
        "dimensionFeed": {
            "type": "string[]",
            "default": []
        },
        "measureFeed": {
            "type": "string[]",
            "default": []
        },
        "caption": {
            "type": "string",
            "description": "",
            "default": ""
        }
    },
    "dataBindings": {
        "myDataSource": {
            "feeds": [
                {
                    "id": "dimensions",
                    "description": "Dimensions",
                    "type": "dimension"
                },
                {
                    "id": "measures",
                    "description": "Measures",
                    "type": "mainStructureMember"
                }
            ]
        }
    },
    "methods": {
        "getCaption": {
            "returnType": "string",
            "description": "The caption of the bar chart",
            "parameters": [],
            "body": "return this.caption;"
        },
        "setCaption": {
            "description": "The caption of the bar chart",
            "parameters": [
                {
                    "name": "caption",
                    "type": "string",
                    "description": "The new caption"
                }
            ],
            "body": "this.caption = caption;"
        },
        "openSelectModelDialog": {
            "parameters": [],
            "body": "this.dataBindings.getDataBinding('myDataSource').openSelectModelDialog();"
        },
        "getDimensions": {
            "parameters": [],
            "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource().getDimensions();",
            "returnType": "DimensionInfo[]"
        },
        "getMeasures": {
            "parameters": [],
            "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource().getMeasures();",
            "returnType": "MeasureInfo[]"
        },
        "addDimension": {
            "parameters": [
                {
                    "name": "dimensionId",
                    "type": "string"
                }
            ]
        },
        "addMeasure": {
            "parameters": [
                {
                    "name": "measureId",
                    "type": "string"
                }
            ]
        },
        "removeDimension": {
            "parameters": [
                {
                    "name": "dimensionId",
                    "type": "string"
                }
            ]
        },
        "removeMeasure": {
            "parameters": [
                {
                    "name": "measureId",
                    "type": "string"
                }
            ]
        },
        "getDimensionsOnFeed": {
            "parameters": [],
            "returnType": "string[]"
        },
        "getMeasuresOnFeed": {
            "parameters": [],
            "returnType": "string[]"
        },
        "getDataSource": {
            "parameters": [],
            "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource();",
            "returnType": "DataSource"
        },
        "setModel": {
            "parameters": [
                {
                    "name": "modelId",
                    "type": "string"
                }
            ],
            "body": "this.dataBindings.getDataBinding('myDataSource').setModel(modelId);"
        }
    },
    "events": {
        "onResultChanged": {
            "description": "called when the result set of the widget changes"
        },
        "onClick": {
            "description": "called when the result set of the widget changes"
        }
    }
}